{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "sql:allow-load",
    "sql:default",
    "sql:allow-execute",
    "shell:default",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "../../public/rust.exe"
        }
      ]
    }
  ]
}
